{
  "extends": [
    "stylelint-config-standard"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "custom-variant",
          "plugin",
          "theme",
          "utility"
        ]
      }
    ],
    "no-descending-specificity": null,
    "selector-class-pattern": null,
    "import-notation": null,
    "at-rule-no-deprecated": null
  },
  "ignoreFiles": [
    "styles/tailwind-output.css"
  ]
}
